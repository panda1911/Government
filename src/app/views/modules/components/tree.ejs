                  <article id="ui-tree" class="mod-doc">
                    <header>
                        <h1 class="title">ui-tree:树结构组件</h1>
                    </header>
                    <div class="content">
                        <section class="overview">
                            <h2 class="cell-title">介绍</h2>
                            <div class="cell-content">
                                树结构控件。
                            </div>
                        </section>
                        <section class="how">
                            <h2 class="cell-title">如何使用</h2>
                            <div class="cell-content">
                                <pre class="sh_javascript">
//简单展示型树结构
$.use('ui-tree',function(){
    var tree = new Energon.tree(options);
});</pre>
                                <pre class="sh_javascript">
//动态加载型树结构
$.use('ui-tree-dynamic',function(){
    var tree = new Energon.dynamicTree(options);
});</pre>
                                <pre class="sh_javascript">
//有关联多选框的动态加载型树结构
$.use('ui-tree-selectable-with-dynamic',function(){
    var tree = new Energon.selectableWithDynamicTree(options);
});</pre>
                                <pre class="sh_javascript">
//无关联多选框的动态加载型树结构
$.use('ui-tree-selectable-with-single',function(){
    var tree = new Energon.selectableWithSingleTree(options);
});</pre>

                            </div>
                        </section>
